<launch>
    <include file="$(find phidgets_imu)/launch/imu.launch" />
    <node pkg="imu_filter_madgwick" type="imu_filter_node" name="ImuFilter">
        <param name="_use_mag" value="false"/>
        <param name="_publish_tf" value="false"/>
        <param name="_world_frame" value="enu"/>
    </node>

    <include file="$(find realsense2_camera)/launch/rs_camera.launch">
         <!--<arg name="align_depth" value="true"/>-->
         <!--<arg name="initial_reset" value="true"/>-->
    </include>

    <param name="velocity_upper" type="int" value="1550" />
    <node name="servo_interface" pkg="autonav" type="servo_interface" output="screen"/>
    <node name="motor_interface" pkg="autonav" type="motor_interface" output="screen"/>
    <node name="ir_node" pkg="autonav" type="ir_node" output="screen"/>
    
    <node name="autonav" pkg="autonav" type="autonav.py" output="screen"/>
    <node name="depth_follow_controller" pkg="autonav" type="depth_follow_controller.py" output="screen"/>
    <node name="stop_sign_detection" pkg="autonav" type="stop_sign_detection.py" output="screen"/>
    <node name="hallway_convergence_controller" pkg="autonav" type="hallway_convergence_controller.py" output="screen"/>
    <node name="ball_detection" pkg="autonav" type="ball_detection.py" output="screen"/>
</launch>
